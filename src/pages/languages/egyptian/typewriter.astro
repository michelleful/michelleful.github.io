    <button id="copy-image-white">Copy as Image (white background)</button>
  </span>
</div>
<div id="copy-status"></div>

<script is:inline define:vars={{ glyphDict, uniControls }}>
  function escapeRegExp(text) {
    return text.replace(/[-[\]{}()*+?.,\\^$|#\s]/g, '\\$&');
  }

  const pattern = new RegExp(`(\\s+|${Object.keys(uniControls).map(escapeRegExp).join('|')})`, 'g');

  function convertMdcToHiero(mdc) {
    const chars = mdc.split(pattern);
    return chars.map(ch => {
      if (ch in glyphDict) return glyphDict[ch];
      if (ch in uniControls) return uniControls[ch];
      return ch;
    }).join('');
  }

  const input = document.getElementById('mdc-input');
  const preview = document.getElementById('preview');
  const copyStatus = document.getElementById('copy-status');

  let currentLines = [];
  let isRtl = false;
  let isVertical = false;

  function getDir() {
    return (isVertical ? 'v' : 'h') + (isRtl ? 'rl' : 'lr');
  }

  // Direction toggle
  document.getElementById('dir-toggle').addEventListener('click', () => {
    isRtl = !isRtl;
    document.getElementById('dir-toggle').textContent =
      isRtl ? 'Direction: RTL \u2190' : 'Direction: LTR \u2192';
    preview.classList.toggle('rtl', isVertical && isRtl);
    update();
  });

  // Orientation toggle
  document.getElementById('orient-toggle').addEventListener('click', () => {
    isVertical = !isVertical;
    document.getElementById('orient-toggle').textContent =
      isVertical ? 'Vertical \u2193' : 'Horizontal \u2192';
    preview.classList.toggle('vertical', isVertical);
    preview.classList.toggle('rtl', isVertical && isRtl);
    update();
  });

  function update() {
    const mdc = input.value.trim();
    if (!mdc) {
      preview.innerHTML = '';
      currentLines = [];
      return;
    }
    const lines = mdc.split('\n').filter(l => l.trim());
    currentLines = lines.map(l => convertMdcToHiero(l.trim()));
    preview.innerHTML = '';
    currentLines.forEach(uni => {
      const span = document.createElement('span');
      span.className = 'hierojax';
      span.setAttribute('data-hierojax', '');
      span.setAttribute('data-type', 'canvas');
      span.setAttribute('data-dir', getDir());
      span.textContent = uni;
      preview.appendChild(span);
    });
    if (typeof hierojax !== 'undefined') {
      preview.querySelectorAll('[data-hierojax]').forEach(el => {
        hierojax.processFragment(el);
      });
    }
  }

  input.addEventListener('input', update);
  update();

  function showStatus(msg) {
    copyStatus.textContent = msg;
    setTimeout(() => { copyStatus.textContent = ''; }, 2000);
  }

  // Copy Unicode text (with controls)
  document.getElementById('copy-unicode').addEventListener('click', () => {
    if (!currentLines.length) { showStatus('Nothing to copy'); return; }
    navigator.clipboard.writeText(currentLines.join('\n')).then(
      () => showStatus('Copied Unicode to clipboard'),
      () => showStatus('Failed to copy')
    );
  });

  // Copy as image (shared logic)
  function copyAsImage(whiteBg) {
    if (!currentLines.length) { showStatus('Nothing to copy'); return; }
    const label = whiteBg ? 'image (white bg)' : 'image';
    try {
      const canvases = [...preview.querySelectorAll('canvas')];
      if (!canvases.length) { showStatus('No canvas found'); return; }
      const pad = 10;
      const gap = 6;
      const c = document.createElement('canvas');
      if (isVertical) {
        const totalWidth = canvases.reduce((sum, s) => sum + s.width, 0) + gap * (canvases.length - 1) + pad * 2;
        const totalHeight = Math.max(...canvases.map(s => s.height)) + pad * 2;
        c.width = totalWidth;
        c.height = totalHeight;
        const ctx = c.getContext('2d');
        if (whiteBg) { ctx.fillStyle = '#ffffff'; ctx.fillRect(0, 0, c.width, c.height); }
        let x = pad;
        const ordered = isRtl ? [...canvases].reverse() : canvases;
        ordered.forEach(src => { ctx.drawImage(src, x, pad); x += src.width + gap; });
      } else {
        const totalWidth = Math.max(...canvases.map(s => s.width)) + pad * 2;
        const totalHeight = canvases.reduce((sum, s) => sum + s.height, 0) + gap * (canvases.length - 1) + pad * 2;
        c.width = totalWidth;
        c.height = totalHeight;
        const ctx = c.getContext('2d');
        if (whiteBg) { ctx.fillStyle = '#ffffff'; ctx.fillRect(0, 0, c.width, c.height); }
        let y = pad;
        canvases.forEach(src => { ctx.drawImage(src, pad, y); y += src.height + gap; });
      }
      c.toBlob(blob => {
        navigator.clipboard.write([new ClipboardItem({ 'image/png': blob })]).then(
          () => showStatus('Copied ' + label + ' to clipboard'),
          () => showStatus('Failed to copy ' + label)
        );
      });
    } catch (e) {
      showStatus('Failed to copy ' + label);
    }
  }

  document.getElementById('copy-image').addEventListener('click', () => copyAsImage(false));
  document.getElementById('copy-image-white').addEventListener('click', () => copyAsImage(true));

  // --- Hieroglyph Picker ---
  let pickerData = null;
  let pickerBuilt = false;
  const overlay = document.getElementById('picker-overlay');
  const pickerBody = document.getElementById('picker-body');
  const pickerSearch = document.getElementById('picker-search');

  // Build reverse mapping: unicode char -> shortest MdC code or Gardiner number
  const uniToCode = {};
  const gardinerPattern = /^([A-Z]+)(\d+)([a-z]*)$/;
  // First pass: collect all Gardiner number keys and MdC alias keys per unicode char
  const charToGardiner = {};
  const charToAliases = {};
  for (const [key, val] of Object.entries(glyphDict)) {
    if (key.includes('-') || key.includes(':') || key.includes('*') || key.includes('\\') || key.includes('<') || key.includes('>') || key.includes('&'))
      continue;
    if (/^\d+$/.test(key)) continue;
    if (gardinerPattern.test(key)) {
      if (!charToGardiner[val]) charToGardiner[val] = [];
      charToGardiner[val].push(key);
    } else {
      if (!charToAliases[val]) charToAliases[val] = [];
      charToAliases[val].push(key);
    }
  }
  // For each unicode char, pick shortest MdC alias if available, else shortest Gardiner
  for (const ch of new Set([...Object.keys(charToGardiner), ...Object.keys(charToAliases)])) {
    const aliases = (charToAliases[ch] || []).sort((a, b) => a.length - b.length || a.localeCompare(b));
    const gardiners = (charToGardiner[ch] || []).sort((a, b) => a.length - b.length || a.localeCompare(b));
    // Prefer shortest alias, fall back to shortest Gardiner number
    uniToCode[ch] = aliases[0] || gardiners[0];
  }

  // Category display names
  const categoryNames = {
    A: 'A - Man and his Activities', B: 'B - Woman and her Activities',
    C: 'C - Anthropomorphic Deities', D: 'D - Parts of the Human Body',
    E: 'E - Mammals', F: 'F - Parts of Mammals', G: 'G - Birds',
    H: 'H - Parts of Birds', I: 'I - Amphibians, Reptiles, etc.',
    K: 'K - Fishes and Parts of Fishes', L: 'L - Invertebrata and Lesser Animals',
    M: 'M - Trees and Plants', N: 'N - Sky, Earth, Water',
    NL: 'NL - Nomes of Lower Egypt', NU: 'NU - Nomes of Upper Egypt',
    O: 'O - Buildings, Parts of Buildings, etc.', P: 'P - Ships and Parts of Ships',
    Q: 'Q - Domestic and Funerary Furniture', R: 'R - Temple Furniture and Sacred Emblems',
    S: 'S - Crowns, Dress, Staves, etc.', T: 'T - Warfare, Hunting, Butchery',
    U: 'U - Agriculture, Crafts, and Professions', V: 'V - Rope, Fibre, Baskets, Bags, etc.',
    W: 'W - Vessels of Stone and Earthenware', X: 'X - Loaves and Cakes',
    Y: 'Y - Writings, Games, Music', Z: 'Z - Strokes and Figures',
    Aa: 'Aa - Unclassified'
  };

  function buildPicker(data) {
    pickerBody.innerHTML = '';
    // Group by category
    const groups = {};
    const catOrder = [];
    for (const sign of data) {
      const uni = sign.unicode;
      if (!uni) continue; // skip unrenderable signs
      const cat = sign.category;
      if (!groups[cat]) { groups[cat] = []; catOrder.push(cat); }
      // Determine insertion code
      const code = uniToCode[uni] || sign.gardiner;
      // Build searchable text
      const searchParts = [
        sign.gardiner.toLowerCase(),
        sign.desc.toLowerCase(),
        ...(sign.phonetic || []).map(p => p.toLowerCase()),
        ...(sign.mdc || []).map(m => m.toLowerCase()),
        ...(sign.words || []).map(w => w.toLowerCase()),
        code.toLowerCase()
      ];
      const searchText = searchParts.join(' ');
      // Build tooltip
      const tipParts = [sign.gardiner + ': ' + sign.desc];
      if (sign.phonetic && sign.phonetic.length) tipParts.push('Phonetic: ' + sign.phonetic.join(', '));
      if (sign.mdc && sign.mdc.length) tipParts.push('MdC: ' + sign.mdc.join(', '));
      const tip = tipParts.join('\n');

      groups[cat].push({ uni, code, gardiner: sign.gardiner, searchText, tip });
    }

    for (const cat of catOrder) {
      const signs = groups[cat];
      // Category header
      const header = document.createElement('div');
      header.className = 'picker-cat-header';
      header.setAttribute('data-cat', cat);
      header.textContent = categoryNames[cat] || cat;
      pickerBody.appendChild(header);
      // Grid
      const grid = document.createElement('div');
      grid.className = 'picker-grid';
      grid.setAttribute('data-cat', cat);
      for (const s of signs) {
        const btn = document.createElement('button');
        btn.className = 'picker-sign';
        btn.setAttribute('data-code', s.code);
        btn.setAttribute('data-search', s.searchText);
        btn.title = s.tip;
        btn.type = 'button';
        btn.innerHTML = '<span class="glyph">' + s.uni + '</span><span class="label">' + s.gardiner + '</span>';
        btn.addEventListener('click', () => { insertSign(s.code); closePicker(); });
        grid.appendChild(btn);
      }
      pickerBody.appendChild(grid);
    }
    pickerBuilt = true;
  }

  function insertSign(code) {
    const ta = document.getElementById('mdc-input');
    const start = ta.selectionStart;
    const end = ta.selectionEnd;
    const text = ta.value;
    let toInsert = code;
    // Auto-prepend separator if needed
    if (start > 0) {
      const before = text[start - 1];
      if (before && before !== '\n' && before !== '-' && before !== ':' && before !== '*') {
        toInsert = '-' + toInsert;
      }
    }
    // Auto-append separator if needed
    if (end < text.length) {
      const after = text[end];
      if (after && after !== '\n' && after !== '-' && after !== ':' && after !== '*') {
        toInsert = toInsert + '-';
      }
    }
    ta.value = text.slice(0, start) + toInsert + text.slice(end);
    const newPos = start + toInsert.length;
    ta.selectionStart = ta.selectionEnd = newPos;
    ta.dispatchEvent(new Event('input'));
    ta.focus();
  }

  // Search with debounce
  let searchTimeout = null;
  pickerSearch.addEventListener('input', () => {
    clearTimeout(searchTimeout);
    searchTimeout = setTimeout(filterPicker, 150);
  });

  function filterPicker() {
    const q = pickerSearch.value.trim().toLowerCase();
    const buttons = pickerBody.querySelectorAll('.picker-sign');
    const grids = pickerBody.querySelectorAll('.picker-grid');
    const headers = pickerBody.querySelectorAll('.picker-cat-header');
    if (!q) {
      buttons.forEach(b => b.hidden = false);
      headers.forEach(h => h.hidden = false);
      grids.forEach(g => g.hidden = false);
      return;
    }
    // Track which categories have visible signs
    const catVisible = {};
    buttons.forEach(b => {
      const match = b.getAttribute('data-search').includes(q);
      b.hidden = !match;
      if (match) {
        const cat = b.closest('.picker-grid').getAttribute('data-cat');
        catVisible[cat] = true;
      }
    });
    headers.forEach(h => { h.hidden = !catVisible[h.getAttribute('data-cat')]; });
    grids.forEach(g => { g.hidden = !catVisible[g.getAttribute('data-cat')]; });
  }

  // Open/close picker
  function openPicker() {
    overlay.hidden = false;
    document.body.style.overflow = 'hidden';
    if (!pickerBuilt && pickerData) {
      buildPicker(pickerData);
    } else if (!pickerData) {
      pickerBody.innerHTML = '<p style="text-align:center;color:#999;padding:2rem;">Loading signs...</p>';
      fetch('/hieroglyphs/sign-metadata.json')
        .then(r => r.json())
        .then(data => {
          pickerData = data;
          buildPicker(data);
        })
        .catch(() => {
          pickerBody.innerHTML = '<p style="color:red;padding:1rem;">Failed to load sign data.</p>';
        });
    }
    setTimeout(() => pickerSearch.focus(), 50);
  }

  function closePicker() {
    overlay.hidden = true;
    document.body.style.overflow = '';
    pickerSearch.value = '';
    if (pickerBuilt) filterPicker(); // reset filter
  }

  document.getElementById('picker-open').addEventListener('click', openPicker);
  document.getElementById('picker-close').addEventListener('click', closePicker);
  overlay.addEventListener('click', (e) => {
    if (e.target === overlay) closePicker();
  });
  document.addEventListener('keydown', (e) => {
    if (e.key === 'Escape' && !overlay.hidden) closePicker();
  });
</script>

</Layout>

